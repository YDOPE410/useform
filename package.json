{
  "name": "forms-builder",
  "version": "0.4.1",
  "description": "Simple and powerful forms for react",
  "main": "dist/index.js",
  "typings": "dist/index",
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^10.5.7",
    "@types/react": "^16.4.8",
    "cross-env": "^5.1.4",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.4.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.json",
    "build": "run-s clean compile",
    "watch": "tsc -w",
    "prepare": "run-s build test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mc-petry/forms-builder.git"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "author": "mc-petry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mc-petry/forms-builder/issues"
  },
  "homepage": "https://github.com/mc-petry/forms-builder#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.test\\.(ts))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}