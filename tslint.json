{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "array-type": [
      false
    ],
    "forin": false,
    "arrow-parens": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "max-classes-per-file": false,
    "max-line-length": [
      false
    ],
    "member-ordering": [
      false
    ],
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-object-literal-type-assertion": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "one-line": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords"
    ],
    "no-implicit-dependencies": [
      false
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "no-bitwise": false,
    "switch-default": false
  }
}